{
  "strict" : true,  // "use strict" を強制
//  "indent" : 2,  // インデントの深さ
  "maxlen" : 80,  // 一行の最大長
  "curly": true, // if文などが中カッコでブロックされているか
  "undef": true, // 明示的に宣言されていない変数の使用を禁止
  "unused" : true,  // 宣言したきり使っていない変数を検出
  "eqeqeq" : true, // ==演算子と!=演算子の使用を禁止
  "eqnull" : true, // == nullに対する警告を出さない。この比較は変数がnullまたはundefinedかどうか調べる際に有用です。
  "expr" : true,  // x || (x = 1); とかができるようにする
  "trailing" : true, // 行末の空白を禁止

  // グローバル変数へのアクセスの管理
  "browser" : true,  // ブラウザ用のやつは許可
  "devel" : true,  //  consoleやalertを許可
  "debug"    : true,  // debugger を許可
  "jquery": true,
  "globals": { // is not defined対策
    "_": false,
    "chrome": true,

    "log"        : true,
    "debug"      : true,
    "info"       : true,
    "warn"       : true,
    "error"      : true,
    "dir"        : true,
    "trace"      : true,
    "assert"     : true,
    "dirxml"     : true,
    "group"      : true,
    "groupEnd"   : true,
    "time"       : true,
    "timeEnd"    : true,
    "count"      : true,
    "profile"    : true,
    "profileEnd" : true,

    "getFile"           : true,
    "setTranslations"   : true,
    "deepCompare"       : true,
    "cloneObject"       : true,
    "getDataType"       : true,
    "getDataURI"        : true,
    "keyCheck"          : true,
    "generateKeyString" : true,
    "toType"            : true,
    "unique"            : true,
    "trim"              : true,
    "arrayEqual"        : true,
    "dictSize"          : true,
    "equals"            : true,

    "translation_path": true,
    "default_values": true,
    "versionKey": true,
    "optionPage": true
  }
}
