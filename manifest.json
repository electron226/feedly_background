{
    "name": "Feedly will open entry in background tab",
    "description": "Feedly will open entry in background tab. default key is 'f'.", 
    "version": "0.3.1",
    "manifest_version": 2,
    "options_page": "options.html",
    "background" : {
      "scripts": ["background.js", "feedly.js"]
    },
    "content_scripts": [ {
      "js": [ "contents.js" ],
      "matches": [
        "http://www.feedly.com/home*",
        "https://www.feedly.com/home*"
      ]
    } ],
    "permissions": [
      "tabs"
    ]
}
