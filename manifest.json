{
    "name": "Feedly will open the entry in background tab",
    "description": "Feedly will the open entry in background tab.", 
    "version": "0.3.6",
    "manifest_version": 2,
    "options_page": "options.html",
    "background" : {
      "scripts": ["background.js", "feedly.js"]
    },
    "content_scripts": [ {
      "js": [ "contents.js" ],
      "matches": [
        "http://*.feedly.com/*",
        "https://*.feedly.com/*"
      ]
    } ],
    "permissions": [
      "tabs"
    ]
}
